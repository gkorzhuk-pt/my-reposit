dev tun
persist-tun
persist-key
data-ciphers CHACHA20-POLY1305:AES-256-GCM:AES-256-CBC
data-ciphers-fallback AES-256-CBC
auth SHA512
tls-client
client
resolv-retry infinite
remote ovpn01.ptsecurity.com 443 tcp4
remote ovpn02.ptsecurity.com 443 tcp4
remote ovpn03.ptsecurity.com 443 tcp4
remote ovpn01.ptsecurity.ru 443 tcp4
remote ovpn02.ptsecurity.ru 443 tcp4
remote ovpn03.ptsecurity.ru 443 tcp4
server-poll-timeout 10
lport 0
auth-user-pass
auth-nocache
verify-x509-name "ovpn.ptsecurity.com" name
remote-cert-tls server
reneg-sec 0
allow-pull-fqdn
verb 4
